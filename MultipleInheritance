package new1;

interface A {
	void show();

}
interface B
{
	void display();
}
interface c extends A,B
{
	void hide();
}
public class New3 implements c
{

	@Override
	public void show() {
		// TODO Auto-generated method stub
		System.out.println("first");
	}

	@Override
	public void display() {
		// TODO Auto-generated method stub
		System.out.println("second");
	}

	@Override
	public void hide() {
		// TODO Auto-generated method stub
		System.out.println("third");
	}

	
		public static void main(String[] args) 
		{
		 c n1=new New3();
		n1.show();
		n1.display();
		n1.hide();
		
		
		}
	}
